{
    "name": "bfe/teams",
    "description": "Laravel package to manage team functionality and operate with user permissions.",
    "keywords": ["laravel", "teams", "gropus", "permissions", "abilities", "auth"],
    "license": "MIT",
    "authors": [
        {
            "name": "Yuri Gerassimov",
            "email": "jurager01@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "illuminate/support": "^8.0|^9.0",
        "illuminate/http": "^8.0|^9.0",
        "illuminate/bus": "^8.0|^9.0",
        "illuminate/mail": "^8.0|^9.0",
        "illuminate/queue": "^8.0|^9.0"
    },
    "require-dev": {
        "laravel/sanctum": "^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Bfe\\Teams\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "stubs/app/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bfe\\Teams\\TeamsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
